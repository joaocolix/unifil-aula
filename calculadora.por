programa{
  funcao inicio() {
    inteiro num1, num2, num3, soma, subtracao
  
    escreva("Digite dois números: ")
    leia(num1, num2, num3)
    soma = (num1 + num2 + num3)
    escreva("A soma é: " + soma + "\n")
    subtracao = (num1 - num2 - num3)
    escreva("A subtraçao é: " + subtracao)

  }
}
